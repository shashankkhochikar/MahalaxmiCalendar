<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@+id/shapeColor">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="@color/white"/>
            <gradient android:startColor="@color/varshikTextcolor" android:endColor="@color/varshikTextcolor" android:angle="90" android:centerColor="@color/varshikTextcolor"/>
            <padding android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_arrow_back_black" android:left="7dp" android:top="7dp" android:right="7dp" android:bottom="7dp"/>
</layer-list>
