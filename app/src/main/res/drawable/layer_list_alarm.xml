<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@+id/shapeColor">
        <shape android:shape="rectangle">
            <padding android:top="3dp" android:bottom="3dp"/>
            <stroke android:width="2dp" android:color="@color/white"/>
            <solid android:color="@color/white"/>
            <solid android:color="@color/white"/>
            <gradient android:startColor="@color/darkBlue" android:endColor="@color/darkBlue" android:angle="90" android:centerColor="@color/darkBlue"/>
            <corners android:radius="15dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/alarm_clock" android:left="8dp" android:top="3dp" android:right="8dp" android:bottom="3dp"/>
</layer-list>
